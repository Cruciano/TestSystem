@model ICollection<BLL.DTO.ResultDto>

<h2>Results</h2>

<table class="table">
    <thead>
        <tr>
            <th>
                UserName
            </th>
            <th>
                Test
            </th>
            <th>
                Score
            </th>
            <th>
                Date
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.UserName
                </td>
                <td>
                    @item.TestTitle
                </td>
                <td>
                    @item.Score
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.DateTime)
                </td>
            </tr>
        }
    </tbody>
</table>

<a asp-controller="Home" asp-action="Index">Back</a>